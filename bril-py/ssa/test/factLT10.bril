# ARGS: live

@main(arg: int) {
  result: int = const 1;
  i: int = id arg;
  ten: int = const 10;

.test:
  isLT10: bool = lt i ten;
  br isLT10 .header .end;

.header:
  # Enter body if i >= 0.
  zero: int = const 0;
  cond: bool = gt i zero;
  br cond .body .end;

.body:
  result: int = mul result i;

  # i--
  one: int = const 1;
  i: int = sub i one;

  jmp .header;

.end:
  print result;
}
